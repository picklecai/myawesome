    "\n",
    "f = open('d:/test.txt','w')\n",
    "\n",
    "for path,dir_list,file_list in g:  \n",
    "    for file_name in file_list:\n",
    "        f.write('\\n')\n",
    "        f.write(os.path.join(path, file_name.replace(u'\\xa0', u'')))\n",
    "f.close()"
